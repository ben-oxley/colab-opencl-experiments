        "import pycuda.driver as cuda\n",
        "import pycuda.autoinit\n",
        "from pycuda.compiler import SourceModule\n",
        "import numpy\n",
        "num_elements = 1000\n",
        "numbers = numpy.random.uniform(0,1000,num_elements)\n",
        "numbers = numbers.astype(numpy.float32)\n"
      ],
      "metadata": {
        "id": "8s3y3kqSgtgL"
      },
      "execution_count": 2,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "mod = SourceModule(\"\"\"\n",
        "  __global__ void doubleit(float *numbers)\n",
        "  {\n",
        "    int idx = (int)threadIdx.x;\n",
        "    numbers[idx] *= 2;\n",
        "  }\n",
        "  \n",
        "  \"\"\")"
      ],
      "metadata": {
        "id": "TDogyjyMgQP6"
      },
      "execution_count": 7,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "numbers_doubled = [num*2 for num in numbers]\n",
        "print(numbers_doubled)"
